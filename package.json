  },
  "homepage": "https://github.com/volkovasystems/diatom#readme",
  "dependencies": {
    "arid": "^0.23.0",
    "babel-runtime": "^6.23.0",
    "budge": "^0.20.0",
    "falzy": "^0.17.0",
    "harden": "^0.21.0",
    "komento": "^0.21.0",
